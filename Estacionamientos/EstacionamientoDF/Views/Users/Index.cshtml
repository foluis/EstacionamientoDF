@model IEnumerable<EstacionamientoDF.ViewModels.UserViewModel>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.First().UserEmail)
        </th>
    </tr>

    @if (Model.Count() == 0)
    {
        <tr>
            <td colspan="4">
                No hay datos.
            </td>
        </tr>
    }
    else
    {
        foreach (var user in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => user.UserEmail)
                </td>
            </tr>
        }
    }
</table>
